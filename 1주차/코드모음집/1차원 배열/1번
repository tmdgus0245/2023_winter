#include <stdio.h>
int main() {
    int T,array[100],s,result=0;
    scanf("%d", &T);
    for (int a=0; a < T; a++) {
        scanf("%d", &array[a]);
    }
    scanf("%d", &s);
    for (int b = 0; b < T; b++) {
        if (s == array[b]) {
            result += 1;
        }
    }
    printf("%d", result);
}
